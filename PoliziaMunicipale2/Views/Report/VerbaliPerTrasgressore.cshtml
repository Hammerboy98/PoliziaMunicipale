@{
    ViewData["Title"] = "Totale dei verbali trascritti raggruppati per trasgressore";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>Trasgressore</th>
            <th>Totale Verbali</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Trasgressore</td>
                <td>@item.TotaleVerbali</td>
            </tr>
        }
    </tbody>
</table>
